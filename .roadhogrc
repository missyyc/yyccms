{
  "entry": "src/index.js",
  "disableCSSModules": true,
  "publicPath": "./",
  "outputPath": "./dist",
  "theme": "./src/theme.js",
  "hash": true,
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  },
  "sass": true
}
